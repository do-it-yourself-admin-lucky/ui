<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/.. \FrameXML\UI.xsd">
 	<Frame name="CT_MapMod_PinTemplate" mixin="CT_MapMod_PinMixin" enableMouseMotion="true" enableMouseClicks="true" virtual="true">
	</Frame>
	<Frame name="CT_MapMod_CornerPinTemplate" mixin="CT_MapMod_CornerPinMixin" hidden="true" virtual="true">
	</Frame>
	<Frame name="CT_MapMod_NoteTemplate" hidden="true" virtual="true">
		<Size>
			<AbsDimension x="330" y="180"/>
		</Size>
		<Backdrop bgFile="Interface/Tooltips/UI-Tooltip-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11"/>
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="16"/>
			</TileSize>
			<EdgeSize>
				<AbsValue val="32"/>
			</EdgeSize>
			<Color a="1" r="0.4" g="0.4" b="0.4"/>
		</Backdrop>
	</Frame>
	<EditBox name="CT_MapMod_EditBoxTemplate" virtual="true">
		<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" tile="true">
			<TileSize>
				<AbsValue val="1"/>
			</TileSize>
		</Backdrop>
		<FontString inherits="ChatFontSmall"/>
	</EditBox>
</Ui>
