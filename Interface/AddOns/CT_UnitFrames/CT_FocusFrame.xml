					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
					<OnMouseDown>
						if ( button == "LeftButton" ) then
							self:StartMoving();
						elseif ( button == "RightButton" ) then
							if (not InCombatLockdown()) then
								CT_UnitFrames_ResetPosition(self:GetName());
								CT_UnitFrames_ResetDragLink(self:GetName());
							end
						end
					</OnMouseDown>
					<OnMouseUp>
